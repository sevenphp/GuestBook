#header{
	width:960px;
	margin:0 auto;
	margin-top:5px;
}

#photo{
	width:960px;
	margin:0 auto;
	border:2px dashed #D6D6D6;
	margin-top:5px;
	padding-bottom:7px;
}

#photo h2{
	text-align:center;
}

#photo dl{
	margin:auto auto;
	width:340px;
	height:480px;
	margin-top:50px;
}

#photo dl dd{
	margin:10px 0;
	padding-left:20px;
}

#photo dl dd#add-photo{
	text-align:center;
	padding-left:0;
}

.text{
	border:1px #F60 dashed;
}

#photo dl dd textarea{
	width:168px;
	height:70px;
}

